@model IEnumerable<Online_Commercial_Authomation.Models.Classes.GroupClass>
@{
    ViewBag.Title = "EasyTables";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>BRTech | Simple Tables</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Admin/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Admin/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <!-- Navbar -->
      
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>Simple Tables</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active">Simple Tables</li>
                            </ol>
                        </div>
                    </div>
                </div><!-- /.container-fluid -->
            </section>

            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6">
                           @Html.Action("Partial4", "Statistics")
                            <!-- /.card -->

                            @Html.Action("Partial1", "Statistics")

                        </div>
                        <!-- /.col -->
                        <div class="col-md-6">
                            <div class="card">
                                <div class="card-header">
                                    <h3 class="card-title">Current City Tables</h3>

                                </div>
                                <!-- /.card-header -->
                                <div class="card-body p-0">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th style="width: 10px">#</th>
                                                <th>City</th>
                                                <th>Progress</th>
                                                <th style="width: 40px">Quantity</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @{ int orderno = 0;}
                                            @foreach (var groupdetail in Model)
                                            {


                                                <tr>
                                                    <td>@{orderno++;} @orderno</td>
                                                    <td>@groupdetail.City</td>
                                                    <td>
                                                        @if (groupdetail.Total == 1)
                                                        {
                                                            <div class="progress progress-xs">
                                                                <div class="progress-bar bg-primary" style="width: 55%"></div>
                                                            </div>
                                                        }
                                                        @if (groupdetail.Total == 2)
                                                        {
                                                            <div class="progress progress-xs">
                                                                <div class="progress-bar bg-warning" style="width: 75%"></div>
                                                            </div>
                                                        }
                                                        @if (groupdetail.Total == 3)
                                                        {
                                                            <div class="progress progress-xs">
                                                                <div class="progress-bar bg-gradient-success" style="width: 55%"></div>
                                                            </div>
                                                        }
                                                        @if (groupdetail.Total == 4)
                                                        {
                                                            <div class="progress progress-xs">
                                                                <div class="progress-bar bg-gradient-pink" style="width: 55%"></div>
                                                            </div>
                                                        }


                                                    </td>
                                                    <td>
                                                        @if (groupdetail.Total == 1)
                                                        {
                                                            <span class="badge bg-primary">@groupdetail.Total</span>
                                                        }
                                                        @if (groupdetail.Total == 2)
                                                        {
                                                            <span class="badge bg-warning">@groupdetail.Total</span>
                                                        }
                                                        @if (groupdetail.Total == 3)
                                                        {
                                                            <span class="badge bg-success">@groupdetail.Total</span>
                                                        }
                                                        @if (groupdetail.Total == 4)
                                                        {
                                                            <span class="badge bg-pink">@groupdetail.Total</span>
                                                        }
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                                <!-- /.card-body -->
                            </div>
                            <!-- /.card -->
                            @Html.Action("Partial5", "Statistics")
                          
                            <!-- /.card -->
                        </div>
                        <!-- /.col -->
                    </div>
                    <!-- /.row -->
                    @Html.Action("Partial2", "Statistics")
                    <!-- /.row -->
                    @Html.Action("Partial3", "Statistics")
                    <!-- /.row -->
                </div><!-- /.container-fluid -->
            </section>
            <!-- /.content -->
   
       
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/Admin/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/Admin/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Admin/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Admin./dist/js/demo.js"></script>
</body>
</html>
